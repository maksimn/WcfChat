<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

  <system.serviceModel>
    <services>
      <service name="WcfChat.Services.AuthService">
        <endpoint address="net.tcp://localhost:8010/AuthService" 
                  binding="netTcpBinding" 
                  contract="WcfChat.Contracts.Service.IAuthService" />
      </service>

      <service name="WcfChat.Services.ChatService">
        <endpoint address="net.tcp://localhost:8010/ChatService" 
                  binding="netTcpBinding" 
                  contract="WcfChat.Contracts.Service.IChatService" />
      </service>
    </services>
  </system.serviceModel>
  
</configuration>